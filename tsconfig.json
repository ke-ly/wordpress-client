{
    "compilerOptions": {
            "target": "ES5",
            "module": "esnext",
            "moduleResolution": "node",
            "importHelpers": true,
            "jsx": "react",
            "lib": ["esnext", "dom"],
            "esModuleInterop": true,
            "sourceMap": true,
            "experimentalDecorators":true,
            "baseUrl": ".",
            "paths": {
            "@/*": ["src/*"]
        },
        "allowSyntheticDefaultImports": true
    }
}
