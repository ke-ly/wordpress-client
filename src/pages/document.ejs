<!doctype html>
<html>
    <head>
        <meta charset="utf-8" />
        <title><%= context.title %></title>
    </head>
    <body>
        <div id="___wp_app"></div>
        <% if (process.env.NODE_ENV == 'production') { %>
        <script type="text/javascript" src="https://unpkg.com/react@16.0.0/umd/react.production.min.js"></script>
        <script type="text/javascript" src="https://unpkg.com/react-dom@16.0.0/umd/react-dom.production.min.js"></script>
        <script type="text/javascript" src="https://unpkg.com/react-router-dom@4.2.0/umd/react-router-dom.min.js"></script>
        <% } else { %>            
            <script type="text/javascript" src="https://unpkg.com/react@16.0.0/umd/react.development.js"></script>
            <script type="text/javascript" src="https://unpkg.com/react-dom@16.0.0/umd/react-dom.development.js"></script>
            <script type="text/javascript" src="https://unpkg.com/react-router-dom@4.2.0/umd/react-router-dom.min.js"></script>
        <% }  %>
    </body>
</html>